{
  "name": "@bytespell/opencode-usage-limits-router",
  "version": "0.0.2",
  "description": "OpenCode plugin to auto-route to best AI provider based on usage limits. Includes CLI.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "opencode-usage-limits": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts ./src/cli.ts ./src/plugin.ts --outdir ./dist --target bun && bun x tsc --emitDeclarationOnly",
    "watch": "bun build ./src/index.ts --outdir ./dist --target bun --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "opencode",
    "plugin",
    "router",
    "usage",
    "limits",
    "anthropic",
    "copilot",
    "openai"
  ],
  "author": "ByteSpell <ashley@bytespell.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bytespell-oss/model-provider-usage-limits.git",
    "directory": "packages/opencode-usage-limits-router"
  },
  "homepage": "https://github.com/bytespell-oss/model-provider-usage-limits#readme",
  "bugs": {
    "url": "https://github.com/bytespell-oss/model-provider-usage-limits/issues"
  },
  "dependencies": {
    "@bytespell/model-provider-usage-limits": "^0.0.1"
  },
  "peerDependencies": {
    "@opencode-ai/plugin": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@opencode-ai/plugin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@opencode-ai/plugin": "^1.1.4",
    "bun-types": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
